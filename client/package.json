{
  "name": "globalauth-client",
  "version": "1.0.0",
  "description": "GlobalAuth client SDK",
  "main": "dist/globalauth.mjs",
  "scripts": {
    "watch": "npx tsc -w",
    "watch:vite": "npx vite build --watch",
    "build": "npx tsc && npx vite build && cp lib/client.d.ts dist/globalauth.d.ts",
    "build:docs": "npx typedoc",
    "build:test": "npm run build && cp dist test/dist -r"
  },
  "author": "0aoq",
  "license": "MIT",
  "devDependencies": {
    "typedoc": "^0.23.14",
    "typescript": "^4.8.3",
    "vite": "^3.1.2"
  }
}
